{
  "name": "react-native-webview-crypto",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/saulshanabrook/react-native-webview-crypto.git"
  },
  "description": "window.crypto for React Native via WebView ",
  "scripts": {
    "prepublish": "npm run typings:install && npm run build:inject && npm run build",
    "typings:install": "typings install",
    "typings:install:test": "cd test; typings install",
    "lint": "tsc",
    "lint:test": "cd test; tsc",
    "build": "webpack",
    "build:inject": "webpack  --config webpack.inject.config.js",
    "test": "cd test; zuul --no-coverage --browser-name ${BROWSER_NAME} --browser-version ${BROWSER_VERSION} --sauce-connect ${TRAVIS_JOB_NUMBER} -- 'test*'",
    "test:local": "cd test; zuul --no-coverage --local -- 'test*'"
  },
  "main": "./dist/WebViewCrypto.js",
  "typings": "./dist/WebViewCrypto.d.ts",
  "files": [
    "dist",
    "src",
    "lib",
    "typings.json"
  ],
  "author": "Saul Shanabrook",
  "license": "MIT",
  "dependencies": {
    "angular2-uuid": "1.0.7",
    "serialize-error": "2.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-webview-bridge": "*"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-react-native": "1.9.0",
    "imports-loader": "0.6.5",
    "lodash": "4.13.1",
    "raw-loader": "0.5.1",
    "react": "15.1.0",
    "react-native": "0.27.2",
    "react-native-webview-bridge": "git+https://github.com/alinz/react-native-webview-bridge.git#d06e80dae2c0900655633fd819eb0a62dc45d56f",
    "source-map-loader": "0.1.5",
    "tape": "4.6.0",
    "ts-loader": "0.8.2",
    "typescript": "1.8.10",
    "typings": "1.3.1",
    "webpack": "1.13.1",
    "webpack-fail-plugin": "1.0.4",
    "zuul": "git+https://github.com/defunctzombie/zuul.git#a8235c288865021a0f32cafcf1ad8ca584e25f90",
    "zuul-builder-webpack": "1.2.0"
  }
}
